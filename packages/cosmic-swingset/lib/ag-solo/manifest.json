{
  "include": [
    "$(MODDABLE)/examples/manifest_base.json",
    "./xs-compartments.json"
  ],
  "creation": {
    "keys": {
      "available": 4096
    },
    "stack": 4096,
    "parser": {
      "buffer": 4096
    },
  },
  "strip": [],
  "preload": [
    "xs-node-global/*"
  ],
  "modules": {
    "main": "../../bin/ag-solo-xs",
    "xs-node-global/*": "../xs-node-global/*",

    "workspace/packages/SwingSet/src/bundles/kernel": "../../../SwingSet/src/kernel-resource",

    "modules/files/file/*": [ "$(MODULES)/files/file/*", "$(MODULES)/files/file/lin/*" ],
    "*":  [
      "$(MODULES)/data/base64/*",
      "$(MODULES)/data/logical/*",
      "$(MODULES)/crypt/digest/*",
      "$(MODULES)/crypt/digest/kcl/*"
    ],
    "socket": [
      "$(MODULES)/network/socket/*",
      "$(MODULES)/network/socket/lin/*"
    ],
  },
  "data": {
    "agoric.kernel": "../../../SwingSet/src/bundles/kernel",
    "@agoric.mailbox-src": "../../../SwingSet/src/bundles/mailbox-src",
    "@agoric.command-src": "../../../SwingSet/src/bundles/command-src",
    "@agoric.timer-src": "../../../SwingSet/src/bundles/timer-src",
    "@agoric.comms": "../../../SwingSet/src/bundles/vat_comms-src",
    "@agoric.vattp": "../../../SwingSet/src/bundles/vat_vattp-src",
    "@agoric.vat-timerWrapper": "../../../SwingSet/src/bundles/vat_timerWrapper-src",
    "xs-compartments": "./xs-compartments"
  }
}
