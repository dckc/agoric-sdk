{
  "include": "$(MODDABLE)/examples/manifest_base.json",
  "creation": {
    "keys": {
      "available": 4096
    },
    "stack": 4096
  },
  "strip": [],
  "preload": [
    "xs-node-global/console",
    "xs-node-global/require-xs",
    "xs-node-global/timer-ticks",
    "tape-promise/tape"
  ],
  "modules": {
    "file": [
      "$(MODULES)/files/file/*",
      "$(MODULES)/files/file/lin/*"
    ],
    "main": "./bin/ag-solo-xs",
    "*":  [
      "lib/xs-node-api/*",
      "lib/xs-npm/*"
    ],

    "xs-node-global/*": "lib/xs-node-global/*",

    "@agoric/harden": "lib/xs-agoric/harden-xs",
    "@agoric/default-evaluate-options": "./lib/xs-agoric/default-evaluate-options",
    "@agoric/bundle-source": "./lib/xs-agoric/bundle-source",

    "@agoric/nat": "../../node_modules/@agoric/nat/dist/nat.esm",
    "@agoric/marshal": "../marshal/marshal",
    "@agoric/eventual-send": "../eventual-send/dist/eventual-send.esm",
    "@agoric/swingset-vat": "../SwingSet/src/index",

    "@agoric/controller": "../SwingSet/src/controller",
    "@agoric/kernel/*": "../SwingSet/src/kernel/*",
    "@agoric/devices/*": "../SwingSet/src/devices/*",
    "@agoric/vats/*": "../SwingSet/src/vats/*",
    "@agoric/vats/comms/*": "../SwingSet/src/vats/comms/*",
    "@agoric/bundles/kernel": "../SwingSet/src/kernel-resource",
    "@agoric/insist": "../SwingSet/src/insist",
    "@agoric/parseVatSlots": "../SwingSet/src/parseVatSlots",
    "@agoric/capdata": "../SwingSet/src/capdata",
    "@agoric/makePromise": "../SwingSet/src/makePromise",
    "@agoric/message": "../SwingSet/src/message",
    "@agoric/kernel/state/*": "../SwingSet/src/kernel/state/*",
    "@agoric/storageAPI": "../SwingSet/src/storageAPI",
    "@agoric/hostStorage": "../SwingSet/src/hostStorage",

    "./lib/_DIRECTORY_MARKER":    "./lib/ag-solo/_DIRECTORY_MARKER",
    "./lib/ag-solo/insist":       "./lib/ag-solo/insist",
    "./lib/ag-solo/start":       "./lib/ag-solo/start",
    "./lib/ag-solo/outbound":       "./lib/ag-solo/outbound",
    "./lib/ag-solo/web":       "./lib/ag-solo-todo/web",
    "./lib/ag-solo/chain-cosmos-sdk":    "./lib/ag-solo-todo/chain-cosmos-sdk",
    "./lib/ag-solo/bundle":       "./lib/ag-solo-todo/bundle",
    "./lib/ag-solo/init-basedir": "./lib/ag-solo-todo/init-basedir",
    "./lib/ag-solo/set-gci-ingress": "./lib/ag-solo-todo/set-gci-ingress",
    "./lib/ag-solo/main":         "./lib/ag-solo/main"
  },

  "data": {
    "agoric.kernel.js": "../SwingSet/src/bundles/kernel",
    "@agoric.mailbox-src": "../SwingSet/src/bundles/mailbox-src",
    "@agoric.command-src": "../SwingSet/src/bundles/command-src",
    "@agoric.timer-src": "../SwingSet/src/bundles/timer-src",
    "@agoric.comms": "../SwingSet/src/bundles/vat_comms-src",
    "@agoric.vattp": "../SwingSet/src/bundles/vat_vattp-src",
    "@agoric.vat-timerWrapper": "../SwingSet/src/bundles/vat_timer-src"
  }
}
